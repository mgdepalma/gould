   <action name="Execute">
    <command>obconf</command>
   </action>
  </item>
  <item icon="/usr/share/gould/pixmaps/launch.png" label="Add Launcher">
   <action name="Execute">
    <command>gpanel -n</command>
   </action>
  </item>
  <item icon="/usr/share/gould/pixmaps/preferences.png" label="Control Panel">
   <action name="Execute">
    <command>gpanel</command>
   </action>
  </item>
  <item icon="/usr/share/gould/pixmaps/cabinet.png" label="File Manager">
   <action name="Execute">
    <command>marlin</command>
   </action>
  </item>
  <item icon="/usr/share/gould/pixmaps/terminal.png" label="Terminal">
   <action name="Execute">
